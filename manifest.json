{

  "manifest_version": 2,
  "name": "Private Keyboard",
  "version": "1.3",


  "description": "Protect against keyboard biometrics",
  "icons": {
    "48": "icons/keyboard-ico.png"
  },

  "permissions": [
    "<all_urls>",
    "storage"
  ],


  "background": {
    "scripts": ["background.js"]
  },

  "options_ui": {
    "page": "settings/options.html"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "keyboardaddon@voidnet.tech"
    }
  }

}
